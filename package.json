{
  "name": "my-personal-blog",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A personal blog built with Astro and Starlight.",
  "author": "Your Name",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "dev": "astro dev --open",
    "start": "pnpm run build && pnpm run preview",
    "build": "NODE_ENV=production astro build",
    "preview": "NODE_ENV=production astro preview --open",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "clean": "rm -rf dist && rm -rf .astro && rm -rf node_modules && pnpm install",
    "scrub": "rm -rf dist && rm -rf .astro && rm -rf node_modules && rm -rf pnpm-lock.yaml && rm -rf ~./.pnpm-store && pnpm install"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/starlight": "^0.33.2",
    "@playform/compress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "astro": "^5.8.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-mdx": "^3.4.0",
    "mermaid": "^11.9.0",
    "node-html-parser": "^7.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "starlight-heading-badges": "^0.6.0",
    "starlight-image-zoom": "^0.13.0",
    "starlight-links-validator": "^0.17.0",
    "typescript": "5.8.3",
    "unist-util-visit": "^5.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "^9.3.0"
      }
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  },
  "devDependencies": {
    "eslint-plugin-mdx": "^3.2.0",
    "vite": "7.0.6"
  },
  "engines": {
    "node": "^20.13.1"
  },
  "engine-strict": true,
  "private": true,
  "sideEffects": false,
  "homepage": "https://github.com/yourusername/your-repo",
  "repository": {
    "type": "git",
    "url": "git@github.com:yourusername/your-repo.git"
  },
  "bugs": "https://github.com/yourusername/your-repo/issues"
}