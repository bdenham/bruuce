{
  "extends": "./config/tsconfig-astro.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./src/content/docs/assets/*"],
      "@docs/*": ["./src/content/docs/*"],
      "@components/*": ["./src/components/*"],
      "@data/*": ["./src/data/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@images/*": ["./public/images/*"]
    },
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["src", "_drafts/_dropins.mdx"]
}
